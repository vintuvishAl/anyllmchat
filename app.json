{
  "expo": {
    "name": "AnyLLMChat",
    "slug": "anyllmchat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/anyllmchatlogo.png",
    "scheme": "anyllmchat",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anyllmchat.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/anyllmchatlogo.png",
        "backgroundColor": "#000322"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anyllmchat.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/anyllmchatlogo.png",
          "imageWidth": 250,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "463a7b9c-8c71-410c-8fb5-1113e9b7bd2b"
      }
    },
    "runtimeVersion": "1.0.0",
    "owner": "vintuvishal",
    "updates": {
      "url": "https://u.expo.dev/463a7b9c-8c71-410c-8fb5-1113e9b7bd2b"
    }
  }
}
